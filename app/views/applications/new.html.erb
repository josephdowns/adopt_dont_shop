<h1>Start an application</h1>

<%= form_with url: "/applications/new", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>
  <%= f.label "Street Address" %>
  <%= f.text_field :street %><br><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br><br>
  <%= f.label :state %>
  <%= f.text_field :state %><br><br>
  <%= f.label "Zip Code" %>
  <%= f.text_field :zip %><br><br>
  <%= f.submit "Submit" %>
<% end %>
